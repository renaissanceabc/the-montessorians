{
  "name": "the-montessorians",
  "type": "module",
  "version": "1.0.0",
  "description": "The largest open source dataset of Montessori alumni",
  "main": "index.js",
  "scripts": {
    "clean": "git clean -xdf node_modules",
    "prepare": "husky",
    "validate": "node scripts/validate.js",
    "lint": "prettier --check \"data/**/*.yaml\" && npm run validate",
    "format": "prettier --write \"data/**/*.yaml\"",
    "bump-deps": "bunx npm-check-updates --deep -u && bun install",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/renaissanceabc/the-montessorians.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/renaissanceabc/the-montessorians/issues"
  },
  "homepage": "https://github.com/renaissanceabc/the-montessorians#readme",
  "devDependencies": {
    "@commitlint/cli": "^20.1.0",
    "@commitlint/config-conventional": "^20.0.0",
    "ajv": "^8.17.1",
    "ajv-cli": "^5.0.0",
    "ajv-formats": "^3.0.1",
    "husky": "^9.1.7",
    "js-yaml": "^4.1.0",
    "prettier": "^3.6.2"
  }
}
